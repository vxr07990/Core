<name>vtiger_survey</name>
<sql><![CDATA[CREATE TABLE `vtiger_survey` (
  `surveyid` int(11) NOT NULL DEFAULT '0',
    `surveyid` VARCHAR(32) DEFAULT NULL,
    `survey_date` DATE DEFAULT NULL,
    `survey_time` TIMESTAMP NULL DEFAULT NULL,
    `surveyor` INT(11) DEFAULT NULL,
    `related_to` INT(11) DEFAULT NULL,
    `contact` INT(11) DEFAULT NULL,
    `potential` INT(11) DEFAULT NULL,
    PRIMARY KEY (`surveyid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
